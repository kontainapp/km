      "dockerignore",
      "efer",
      "ehdr",
      "eventfd",
      "ffffffffful",
      "ffffffful",
      "getehdr",
      "hcalls",
      "hwcap",
      "infox",
      "kubernetes",
      "memsz",
      "mmaps",
      "mprotect",
      "musl",
      "pdpe",
      "pdpte",
      "phnum",
      "pidof",
      "ptcb",
      "runc",
      "shdr",
      "sregs",
      "subdir",
      "vcpu",
      "vmexit",
      "vpcu",
      "withdocker",
      "xbffffffffffffffful",
